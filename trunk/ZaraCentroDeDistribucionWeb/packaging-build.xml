<?xml version="1.0" encoding="UTF-8"?>
<project name="Packaging Generator" default="_packaging_generation_">
<target name="_packaging_generation_" depends="N65540,N65565,N65615,N65654"/>
<target name="N65540" description="ServerEJB.jar">
<mkdir dir="dist"/>
<jar destfile="dist/ServerEJB.jar">
<zipfileset dir="." prefix="META-INF">
<include name="persistence.xml"/>
</zipfileset>
<zipfileset dir="bin" includes="**/*.class"/>
</jar>
</target>
<target name="N65565" description="ServerWEB.war">
<mkdir dir="dist"/>
<jar destfile="dist/ServerWEB.war">
<zipfileset dir=".">
<include name="struts-config.xml"/>
</zipfileset>
<zipfileset dir="." prefix="WEB-INF">
<include name="web.xml"/>
</zipfileset>
<zipfileset dir="paginas"/>
<zipfileset dir="bin" includes="struts/actions/*.class,struts/forms/*.class,struts/model/*.class"/>
<zipfileset dir="" includes="*.tld"/>
</jar>
</target>
<target name="N65615" description="ServerApp.ear">
<mkdir dir="dist"/>
<jar destfile="dist/ServerApp.ear">
<zipfileset dir="dist">
<include name="ServerEJB.jar"/>
</zipfileset>
<zipfileset dir="dist">
<include name="ServerWEB.war"/>
</zipfileset>
<zipfileset dir="." prefix="META-INF">
<include name="application.xml"/>
</zipfileset>
<zipfileset dir="dist">
<include name="WebServices.jar"/>
</zipfileset>
</jar>
</target>
<target name="N65654" description="WebServices.jar">
<mkdir dir="dist"/>
<jar destfile="dist/WebServices.jar">
<zipfileset dir="bin/webservices" includes="Servicios.class, ServiciosSoapBindingSkeleton.class, ServiciosSoapBindingImpl.class"/>
</jar>
</target>
</project>
