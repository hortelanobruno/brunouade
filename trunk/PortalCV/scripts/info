//Query lista de sessiones
select username,FramedIPAddress,AcctStartTime,AcctStopTime,useragent,logintype from radacct where useragent is not null and ciscoserviceinfo = '' order by AcctStartTime desc limit 100;


// Query check WLCACCT multiples rows
select count(*) as cant,IPCPE from portalcv_wlcaccounting where acctStop is null group by IPCPE having cant>1;


